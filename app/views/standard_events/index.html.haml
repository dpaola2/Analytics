%ol.breadcrumb
  %li.active Events
.span8
  .panel.panel-default
    .panel-heading
      Events Overview (last 30 days)
    .panel-body
      = line_chart event_summary_path
    %ul.list-group
      - @standard_events.each do |event|
        = link_to event.name, standard_event_path(event), :class => "list-group-item"
